{
  "name": "metamask.synpresstests",
  "version": "1.0.0",
  "description": "example of synpress tests",
  "main": "index.js",
  "scripts": {
    "test": "synpress run --configFile synpress.config.js --env METAMASK_VERSION=%METAMASK_VERSION%,RUN_ID=%RUN_ID%,PRIVATE_KEY=%PRIVATE_KEY%"
  },
  "reporter": "mochawesome",
  "reporterOptions": {
    "overwrite": false,
    "html": false,
    "json": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TourmalineCore/Metamask.SynpressTests.git"
  },
  "devDependencies": {
    "@robertw07/synpress": "^0.0.6",
    "cypress": "^12.7.0",
    "mochawesome": "^7.1.3"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "fs": "^0.0.1-security"
  }
}
